{
  "sections": {
    "main": {
      "type": "main-page",
      "disabled": true,
      "settings": {
        "padding_top": 100,
        "padding_bottom": 100
      }
    },
    "6e982dbf-9ca1-4520-ab7e-d2492fec3245": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"page-width blo-collection-list\">\n  <h1 class=\"title title--primary inline-richtext{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}\">\n    {{ page.title }}\n  <\/h1>\n\n  {%- paginate collections by 100 -%}\n    <ul\n      class=\"collection-list grid grid--{{ section.settings.columns_desktop }}-col-desktop grid--{{ section.settings.columns_mobile }}-col-tablet-down\"\n      role=\"list\"\n    >\n      {%- for collection in collections -%}\n\n          {% if collection.template_suffix == \"style\"  %} \n            <li\n            class=\"collection-list__item grid__item{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}\"\n            {% if settings.animations_reveal_on_scroll %}\n              data-cascade\n              style=\"--animation-order: {{ forloop.index }};\"\n            {% endif %}\n          >\n              {% render 'card-collection',\n                card_collection: collection,\n                media_aspect_ratio: section.settings.image_ratio,\n                columns: 3\n              %}\n          <\/li>\n          {% else %}\n          {% endif %}     \n        \n      {%- endfor -%}\n    <\/ul>\n    {% render 'pagination', paginate: paginate %}\n  {%- endpaginate -%}\n<\/div>",
        "color_scheme": "",
        "padding_top": 100,
        "padding_bottom": 100
      }
    }
  },
  "order": [
    "main",
    "6e982dbf-9ca1-4520-ab7e-d2492fec3245"
  ]
}
